{
  "manifest_version": 3,
  "name": "Tab Groups Saver",
  "version": "1.0",
  "description": "Base Level Extension",
  "permissions": [
    "tabs",
    "tabGroups",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "src/pages/new-tab/index.html"
  },
  "action": {
    "default_popup": "src/pages/popup/index.html"
  }
}
