{
  "name": "tab-groups-saver",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "NODE_ENV=development vite build --watch",
    "dev-sidecar": "NODE_ENV=dev vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "build-only": "vite build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "test:types": "vue-tsc --build",
    "test:lint": "eslint .",
    "test:format": "prettier --check src/",
    "test": "npx concurrently \"npm run test:lint\" \"npm run test:format\" \"npm run test:types\"",
    "test-linear": "npm run test:lint && npm run test:format && npm run test:types"
  },
  "dependencies": {
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.2",
    "@types/chrome": "^0.1.24",
    "@types/node": "^22.16.5",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.7.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.31.0",
    "eslint-plugin-vue": "~10.3.0",
    "hot-reload-extension-vite": "^1.0.13",
    "jiti": "^2.4.2",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.6.2",
    "sass-embedded": "^1.92.1",
    "typescript": "~5.8.0",
    "vite": "^7.0.6",
    "vite-plugin-static-copy": "^3.1.2",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vite-svg-loader": "^5.1.0",
    "vue-tsc": "^3.0.4"
  }
}
